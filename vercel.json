{
    "version": 2,
    "builds": [
      {
        "src": "src/index.ts",
        "use": "@vercel/node"
      },
      {
        "src": "dist/public/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "src/index.ts"
      },
      {
        "src": "/(.*)",
        "dest": "/dist/public/$1"
      }
    ]
  }
  